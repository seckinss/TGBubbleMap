{
  "name": "bubblemap-image-generator",
  "version": "1.0.0",
  "description": "A service that generates bubble map visualizations from the Bubblemaps legacy API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:bot": "node dist/src/bot.js",
    "dev": "ts-node-esm index.ts",
    "dev:bot": "NODE_OPTIONS=\"--loader ts-node/esm\" node --experimental-specifier-resolution=node src/bot.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "bubblemaps",
    "visualization",
    "blockchain",
    "token",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsdom": "^24.0.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.30",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
